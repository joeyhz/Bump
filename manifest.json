{
  "name": "Bump",
  "version": "1.0",
  "description": "Make streaming a social experience!",
  "manifest_version": 2,
  "options_page": "options.html",
  "oauth2":{
      "client_id":"285182200858-a3p61ck2fqume19a859elnecuvvt9o57.apps.googleusercontent.com",
      "scopes":["https://www.googleapis.com/auth/spreadsheets"]
  },
  "permissions": ["*://*/*","tabs","declarativeContent","storage", "identity", "identity.email", "webNavigation"],
  "content_security_policy": "script-src 'self' https://*.google.com/ https://unpkg.com/sweetalert/ https://www.gstatic.com/ https://*.firebaseio.com https://*.firebase.com https://www.googleapis.com; object-src 'self'; connect-src 'self' wss://*.firebaseio.com https://*.googleapis.com https://*.facebook.com;",
  "key":"",
  "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/logo16.png",
          "32": "images/logo32.png",
          "48": "images/logo48.png",
          "128": "images/logo128.png"
      }
    },
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    },
      "background": {
        "page": "background.html",
        "persistent": false
  }
}
